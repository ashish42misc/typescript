{
    "compilerOptions": {
        "target":"ES2015",
        "module": "CommonJS",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true
        
    },
    "exclude": [
        "node_modules"
    ]
}